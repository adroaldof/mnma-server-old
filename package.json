{
  "name": "mnma",
  "version": "0.0.1",
  "description": "Mnma Server",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/nodemon --exec npm run dev",
    "dev": "babel-node index.js",
    "up": "docker-compose up"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/mnma/mnma-server.git"
  },
  "keywords": [
    "Mnma",
    "server"
  ],
  "author": "Adroaldo de Andrade <adroaldof@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/mnma/mnma-server/issues"
  },
  "homepage": "https://gitlab.com/mnma/mnma-server#README",
  "engine": {
    "node": "7.6.0"
  },
  "babel": {
    "plugins": [
      "syntax-object-rest-spread",
      "transform-async-to-generator",
      "transform-es2015-destructuring",
      "transform-es2015-modules-commonjs",
      "transform-export-extensions",
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "express": "^4.15.3",
    "kerberos": "0.0.23",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "parse": "^1.10.0",
    "parse-dashboard": "^1.0.28",
    "parse-server": "^2.5.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.2"
  }
}
